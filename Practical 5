#include <stdio.h>  
#define SIZE 10   // size of hash table  
int main() {    
 int 
hashTable[SIZE];    
i, key, index;  
 int 
// Initialize hash table     
for (i = 0; i < SIZE; i++) {  
hashTable[i] = -1;  // -1 means empty slot  
}  
int keys[] = {23, 43, 13, 27};  // some sample keys     
int n = 4;  
// Insert keys into hash table     
0; i < n; i++) {         
key = keys[i];         
for (i = 
index = key % SIZE;  // hash function  
// Linear probing for collision         
while (hashTable[index] != -1) {             
index = (index + 1) % SIZE; 
        }  
hashTable[index] = key;  
}  
// Display hash table     
printf("Index\tValue\n");     for 
(i = 0; i < SIZE; i++) {  
printf("%d\t%d\n", i, hashTable[i]);  
}  
return 0;  
} 
